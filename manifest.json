{
    "manifest_version": 2,
    "name": "Forms Autocomplete",
    "description": "Your inputs are autofill after you define initial values.",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "bookmarks",
        "unlimitedStorage"
      ]
}