{
    "manifest_version": 2,
    "name": "Forms Autocomplete",
    "description": "Your inputs are autofill after you define initial values.",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Test",
        "default_popup": "popup.html"
    },
    
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "bookmarks",
        "unlimitedStorage"
    ]
}